    },
    {
      "name": "job_tracker-{prod}",
      "request": "launch",
      "program": "lib/main_prod.dart",
      "type": "dart",
      "args": ["--flavor", "prod", "--no-enable-impeller"]
    },
    {
      "name": "job_tracker (profile mode)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "flutterMode": "profile"
